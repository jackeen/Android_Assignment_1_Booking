<!-- res/drawable/star_empty.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size android:width="20dp" android:height="20dp"/> <!-- Change width and height for size -->
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="2dp" android:color="@android:color/darker_gray"/> <!-- Outline color -->
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>
